{
  "name": "quick-star",
  "displayName": "Quick Star",
  "icon": "sources/images/quick_star_icon.jpg",
  "description": "Transform selected text using LLM API with customizable rules",
  "version": "1.0.0",
  "publisher": "woodx9",
  "repository": {
    "type": "git",
    "url": "https://github.com/quickstar/vscode-extension"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quickstar.transformText"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quickstar.transformText",
        "title": "Transform Text with LLM"
      },
      {
        "command": "quickstar.createSampleConfig",
        "title": "Quick Star: Create Sample Configuration"
      }
    ],
    "keybindings": [
      {
        "command": "quickstar.transformText",
        "key": "ctrl+g",
        "mac": "cmd+g",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "license": "MIT"
}
